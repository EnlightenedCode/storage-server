<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>rva-test</application>
  <module>medialibrary</module>
  <version>1-01-028</version>
  <instance-class>F1</instance-class>
  <precompilation-enabled>true</precompilation-enabled>
  <ssl-enabled>true</ssl-enabled>
  <threadsafe>true</threadsafe>
  <automatic-scaling>
    <min-idle-instances>automatic</min-idle-instances>
    <max-idle-instances>automatic</max-idle-instances>
    <min-pending-latency>automatic</min-pending-latency>
    <max-pending-latency>30ms</max-pending-latency>
  </automatic-scaling>

  <!-- Configure java.util.logging -->
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
  </system-properties>

  <admin-console>
    <page name="Appstats" url="/appstats" />
  </admin-console>

<!--   <public-root>index</public-root> -->

</appengine-web-app>
